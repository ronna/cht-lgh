      <bind nodeset="/scdsc_investigation/scdsc_investigation" required="true()"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/signal_id_reminder" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/signal_investigated" required="true()" type="select1"/>
      <bind nodeset="/scdsc_investigation/scdsc_investigation/cause" relevant=" /scdsc_investigation/scdsc_investigation/signal_investigated  = 'yes'" required="true()" type="string"/>
