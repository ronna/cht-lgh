            <s_investigation_exposed/>
            <s_investigation_suspected/>
            <s_investigation_deaths/>
            <s_investigation_hospitalized/>
            <s_event_risk_classification/>
            <s_investigation_risk/>
            <s_followup/>
            <s_followup_note_1/>
