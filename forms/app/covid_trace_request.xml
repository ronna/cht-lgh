      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_label" readonly="true()" relevant="selected(../dob_method,'approx')" type="string"/>
      <bind constraint=". &gt;= 0 and . &lt;= 130" jr:constraintMsg="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_years:jr:constraintMsg')" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_years" relevant="selected(../dob_method,'approx')" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_months:jr:constraintMsg')" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_months" relevant="selected(../dob_method,'approx')" type="int"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_method" type="select"/>
      <bind calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%m&quot;) - ../age_months + 12, format-date-time(today(),&quot;%m&quot;) - ../age_months)" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_months" type="string"/>
      <bind calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%Y&quot;) - ../age_years - 1, format-date-time(today(),&quot;%Y&quot;) -../age_years)" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_years" type="string"/>
      <bind calculate="concat(string( /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_years ),'-',if( /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_months &lt;10, concat('0',string( /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_months )),  /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/ephemeral_months ),'-',string(format-date-time(today(), &quot;%d&quot;)))" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_approx" type="string"/>
      <bind calculate="if(not(selected(  /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_method ,'approx')), 
 /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_calendar ,
 /covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_approx )" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_raw" type="string"/>
      <bind calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_iso" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/phone" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/flight_details" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/address" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/town" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/village" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/chief_name" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/chief_phone" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/church" type="string"/>
      <bind calculate=" /covid_trace_request/patient_id " nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/covid_patient" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/tracer_area" type="db:clinic"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent/_id" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent/parent/_id" type="string"/>
      <bind nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent/parent/parent/_id" type="string"/>
      <bind calculate="../../tracer/tracer_area" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/parent/_id" type="string"/>
      <bind calculate="../../../tracer/parent/_id" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/parent/parent/_id" type="string"/>
      <bind calculate="../../../../tracer/parent/parent/_id" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/parent/parent/parent/_id" type="string"/>
      <bind calculate="&quot;person&quot;" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/type" type="string"/>
      <bind calculate="&quot;contact_n&quot;" nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/role" type="string"/>
      <bind calculate="." nodeset="/covid_trace_request/contacts/contact_repeat/contact_profile/created_by_doc" type="string"/>
      <bind calculate=" /covid_trace_request/contacts/contact_repeat/contact_profile " nodeset="/covid_trace_request/contacts/contact_repeat/contact_doc" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/covid_trace_request/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/covid_trace_request/inputs">
      <label ref="jr:itext('/covid_trace_request/inputs:label')"/>
      <input appearance="hidden" ref="/covid_trace_request/inputs/source">
        <label ref="jr:itext('/covid_trace_request/inputs/source:label')"/>
      </input>
      <input appearance="hidden" ref="/covid_trace_request/inputs/source_id">
        <label ref="jr:itext('/covid_trace_request/inputs/source_id:label')"/>
      </input>
      <group ref="/covid_trace_request/inputs/user">
        <label ref="jr:itext('/covid_trace_request/inputs/user:label')"/>
        <input appearance="db-object" ref="/covid_trace_request/inputs/user/contact_id">
          <label ref="jr:itext('/covid_trace_request/inputs/user/contact_id:label')"/>
        </input>
      </group>
      <group ref="/covid_trace_request/inputs/contact">
        <label ref="jr:itext('/covid_trace_request/inputs/contact:label')"/>
        <input appearance="hidden" ref="/covid_trace_request/inputs/contact/_id">
          <label ref="jr:itext('/covid_trace_request/inputs/contact/_id:label')"/>
        </input>
        <group ref="/covid_trace_request/inputs/contact/parent">
          <input appearance="hidden" ref="/covid_trace_request/inputs/contact/parent/_id">
            <label ref="jr:itext('/covid_trace_request/inputs/contact/parent/_id:label')"/>
          </input>
          <group ref="/covid_trace_request/inputs/contact/parent/parent">
            <input appearance="hidden" ref="/covid_trace_request/inputs/contact/parent/parent/_id">
              <label ref="jr:itext('/covid_trace_request/inputs/contact/parent/parent/_id:label')"/>
            </input>
            <group ref="/covid_trace_request/inputs/contact/parent/parent/parent">
              <input appearance="hidden" ref="/covid_trace_request/inputs/contact/parent/parent/parent/_id">
                <label ref="jr:itext('/covid_trace_request/inputs/contact/parent/parent/parent/_id:label')"/>
              </input>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/covid_trace_request/contact_info">
      <label ref="jr:itext('/covid_trace_request/contact_info:label')"/>
      <input ref="/covid_trace_request/contact_info/n_case_id">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_case_id:label')"/>
      </input>
      <input ref="/covid_trace_request/contact_info/n_surname">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_surname:label')"/>
      </input>
      <input ref="/covid_trace_request/contact_info/n_other_names">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_other_names:label')"/>
      </input>
      <input ref="/covid_trace_request/contact_info/n_phone">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_phone:label')"/>
      </input>
      <input ref="/covid_trace_request/contact_info/n_address">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_address:label')"/>
      </input>
      <input ref="/covid_trace_request/contact_info/n_town">
        <label ref="jr:itext('/covid_trace_request/contact_info/n_town:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/covid_trace_request/contacts">
      <input ref="/covid_trace_request/contacts/n_contacts">
        <label ref="jr:itext('/covid_trace_request/contacts/n_contacts:label')"/>
      </input>
      <group ref="/covid_trace_request/contacts/contact_repeat">
        <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat:label')"/>
        <repeat nodeset="/covid_trace_request/contacts/contact_repeat">
          <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile">
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/surname">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/surname:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/other_names">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/other_names:label')"/>
            </input>
            <select1 ref="/covid_trace_request/contacts/contact_repeat/contact_profile/sex">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/sex:label')"/>
              <item>
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/sex/female:label')"/>
                <value>female</value>
              </item>
              <item>
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/sex/male:label')"/>
                <value>male</value>
              </item>
            </select1>
            <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob">
              <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_calendar">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_calendar:label')"/>
                <hint ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_calendar:hint')"/>
              </input>
              <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_label">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_label:label')"/>
              </input>
              <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_years">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_years:label')"/>
              </input>
              <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_months">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/age_months:label')"/>
              </input>
              <select appearance="horizontal" ref="/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_method">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_method:label')"/>
                <item>
                  <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/ephemeral_dob/dob_method/approx:label')"/>
                  <value>approx</value>
                </item>
              </select>
            </group>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/phone">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/phone:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/flight_details">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/flight_details:label')"/>
              <hint ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/flight_details:hint')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/address">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/address:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/town">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/town:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/village">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/village:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/chief_name">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/chief_name:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/chief_phone">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/chief_phone:label')"/>
            </input>
            <input ref="/covid_trace_request/contacts/contact_repeat/contact_profile/church">
              <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/church:label')"/>
            </input>
            <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer">
              <input appearance="db-object" ref="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/tracer_area">
                <label ref="jr:itext('/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/tracer_area:label')"/>
              </input>
              <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent">
                <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent/parent">
                  <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/tracer/parent/parent/parent"/>
                </group>
              </group>
            </group>
            <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/parent">
              <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/parent/parent">
                <group ref="/covid_trace_request/contacts/contact_repeat/contact_profile/parent/parent/parent"/>
              </group>
            </group>
          </group>
        </repeat>
      </group>
    </group>
  </h:body>
</h:html>
